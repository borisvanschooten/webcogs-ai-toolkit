{
  "name": "webcogs",
  "displayName": "WebCogs",
  "description": "Experimental AI toolkit",
  "repository": "https://github.com/borisvanschooten/webcogs-ai-toolkit",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Enable Webcogs CodeLens",
        "command": "webcogs.enableCodeLens",
        "category": "Webcogs"
      },
      {
        "title": "Disable Webcogs Codelens",
        "command": "webcogs.disableCodeLens",
        "category": "Webcogs"
      },
      {
        "title": "Re-enter Webcogs OpenAI API key",
        "command": "webcogs.enterOpenAIAPiKey",
        "category": "Webcogs"
      }
    ],
    "configuration": {
      "properties": {
        "webcogs.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "install-local-webcogs": "cd ../.. && npm run pack && cd vscode-extension/webcogs/ && npm install ../../webcogs-0.2.0.tgz --no-save"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
